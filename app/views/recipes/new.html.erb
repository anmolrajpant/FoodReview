<%= form_for :recipe, url: {action: 'create'}, :html => {:multipart => true} do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %><br>
	<%= f.label :items %>
	<%= f.text_area :items %><br>
	<%= f.label :process %>
	<%= f.text_area :process %><br>
	<%= f.label :images %>
	<%= f.file_field :image %>
	<%= submit_tag ("Save") %>
<% end %>
